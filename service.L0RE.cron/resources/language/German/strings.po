msgctxt "#30521"
msgid "Crontab for other Addons:"
msgstr "Crontab für andere Addons:"

msgctxt "#30522"
msgid "[COLOR FF008B00]* cron.db *[/COLOR] successfully deleted !"
msgstr "[COLOR FF008B00]* cron.db *[/COLOR] erfolgreich gelöscht !"

